using System;
using System.Collections.Generic;
using System.Linq;
using System.Web;
using Hatfield.WebMap.Domain;

namespace Hatfield.WebMap.Repositories
{    

    public class UserRepository : BaseJSONDataFileRepository<WebMapUser>
    {
        public override string GetDataFileLocationOnDisk()
        {
            return System.Web.Hosting.HostingEnvironment.MapPath("~/App_Data/UserRepository.json");
        }
    }
}