using System;
using System.Collections.Generic;
using System.Linq;
using System.Web;

namespace Hatfield.WebMap.Repositories
{
    public class DUOrigionalLimit
    {
        public int OBJECTID { get; set; }        
        public string DU_ID { get; set; }
        public Nullable<decimal> OrigionalLimit { get; set; }
    }

    public class DUOrigionalLimitRepository : BaseJSONDataFileRepository<DUOrigionalLimit>
    {
        public override string GetDataFileLocationOnDisk()
        {
            return System.Web.Hosting.HostingEnvironment.MapPath("~/App_Data/DUOrigionalLimitRepository.json");
        }
    }
}